pipeline {
 agent any
    stages {
        stage ('Compile Stage') {
             steps {
                 echo 'Hello compile stage'
            }
        }
        stage ('Testing Stage') {
            steps {
                echo 'hello testing stage'
            }
        }
        stage ('Deployment Stage') {
            steps {
                    echo 'deploy stage'
            }
        }
    }
}
